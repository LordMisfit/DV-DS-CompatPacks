ACTOR GoreBalloonNew : Circus2Weapon replaces GoreBalloon 30030
{
	Weapon.SelectionOrder 1500
	Inventory.PickupSound "misc/w_pkup" 
	Weapon.AmmoType "GoreBalloons" 
	Weapon.AmmoGive 5 
	Weapon.AmmoUse 1
	Inventory.Pickupmessage "You got some gore balloons!" 
	Obituary "%o wanted %k's balloon."
	Scale 0.75
	States 
	{
		Spawn: 
			BLNN J -1 
			loop
		Ready: 
			TNT1 A 0 A_JumpIf(CallACS("CheckCharacterClass") == 2, 6)
			BLNN ABCD 6 A_WeaponReady(WRF_ALLOWUSER1) 
			TNT1 A 0 A_JumpIf(CallACS("CheckCharacterClass") != 2, 5)
			BLNF ABCD 6 A_WeaponReady(WRF_ALLOWUSER1)
			TNT1 A 0
			loop
			TNT1 A 0 A_JumpIf(CallACS("CheckCharacterClass") == 2, 4)
			BLNN AA 1 
			TNT1 A 0 A_JumpIf(CallACS("CheckCharacterClass") != 2, 3)
			BLNF AA 1 
			TNT1 A 0
			Goto Deselect
		Deselect: 
			TNT1 A 0 A_JumpIf(CallACS("CheckCharacterClass") == 2, 6)
			BLNN EEEE 1 A_Lower 
			TNT1 A 0 A_JumpIf(CallACS("CheckCharacterClass") != 2, 5)
			BLNF EEEE 1 A_Lower 
			TNT1 A 0
			Loop
		Select: 
			TNT1 A 0 A_JumpIf(CallACS("CheckCharacterClass") == 2, 6)
			BLNN EEEE 1 A_Raise 
			TNT1 A 0 A_JumpIf(CallACS("CheckCharacterClass") != 2, 5)
			BLNF EEEE 1 A_Raise 
			TNT1 A 0
			loop
		Fire: 
			TNT1 A 0 A_JumpIf(CallACS("CheckCharacterClass") == 2, 5)
			BLNN A 1 
			BLNN FG 2
			TNT1 A 0 A_JumpIf(CallACS("CheckCharacterClass") != 2, 4)
			BLNF A 1 
			BLNF FG 2
			TNT1 A 0
			{
				CallACS("StartTensionCountDown");
				A_SpreadAuraFireProjectileAoE("GoreBalloon666","GoreBalloon666NoHurt",0,1,"GoreBalloons",4,1,0,0);
			}
			TNT1 A 0 A_JumpIf(CallACS("CheckCharacterClass") == 2, 5)
			BLNN I 6
			BLNN IH 6
			TNT1 A 0 A_JumpIf(CallACS("CheckCharacterClass") != 2, 4)
			BLNF I 6
			BLNF IH 6
			TNT1 A 0 
			Goto Select
	}
}
