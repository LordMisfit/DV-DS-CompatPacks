}

//this was originally unused; I used it in "LightningColumns"
ACTOR LightningRod3
{
  Radius 24
  Height 8
  Damage 5
  Speed 20
  RENDERSTYLE Add
  Alpha 0.40
  SEESOUND "thor/spawnl"
  DEATHSOUND "thor/hitl"
