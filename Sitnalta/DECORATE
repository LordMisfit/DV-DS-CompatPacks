/////Zakachi Decorate\\\\\

ACTOR Zakachi 27043
{
 var int user_theta; var int user_dormant; var int user_isstealth; var int user_basespeed; var int user_baseattack; var int user_basedefense; var int user_basemass; var int user_monsterlevel;
 var int user_MaxHP; var int user_HPLowRange; var int user_HPCritRange; var int user_HPBuffRange; var int user_HPXDeathRange;
 var int user_checkmapskill; var int user_ressurrectstr; // For checking if a monster has been given Skill/Map inventory items yet.

  OBITUARY "%o witnessed the true power of Zakachi Daemonbourne."
  Health 48000
  Radius 25
  Height 75//50
  Mass 200
  Speed 12//4
  PainChance 25//100
  SEESOUND "thor/sight"
  PAINSOUND "thor/pain"
  DEATHSOUND "thor/death"
  ACTIVESOUND "thor/active"
  BloodColor "00 00 20"
  MONSTER
  +Boss
  +FloorClip
  +NoRadiusDMG
  +DontMorph
  +MissileMore
  +NoIceDeath
  states
  {
  Spawn:
    TNT1 A 0 A_Look
    THOR M 0 A_CustomMissile("LightningBallC1",200,0,0,2,0)
    THOR M 0 A_CustomMissile("LightningBallC2",200,0,180,2,0)
	Goto Idle
  Idle:
    TNT1 A 0 A_Look
    TNT1 A 0 ACS_NamedExecuteAlways("MonsterStatsHP_ATK_DEF_SPD",0,0)
	THOR M 10 A_Look
    Loop
  See:
    TNT1 A 0 ACS_NamedExecuteAlways("MonsterStatsHP_ATK_DEF_SPD",0,0)
    TNT1 A 0 Thing_ChangeTID(0,1222)
    THOR MNOP 5 A_Chase
    Loop
  Missile:
    TNT1 A 0 ACS_NamedExecuteAlways("RefreshStealthInfo",0,1) // makes a Stealth Monster become more visible
	TNT1 A 0 A_PlaySound("thor/attack")
    TNT1 A 0 A_Jump(64, "LightningColumns")
    TNT1 A 0 A_Jump(96, "GroundSeekers")
    TNT1 A 0 A_Jump(64, "Filibuster")
    TNT1 A 0 A_Jump(128, "LightningStream")
	Goto LightningBall
//attack 1; fragmenting lightning ball
  LightningBall:
    THOR R 5
	THOR S 5 A_FaceTarget
    THOR T 5 BRIGHT A_CustomMissile("LightningBallL",32,0,0,2,0)
	THOR T 5
    Goto See
//attack 2; lightning streams
  LightningStream:
    THOR RS 5 A_FaceTarget
	THOR T 5 A_FaceTarget
    THOR T 1 BRIGHT A_CustomMissile("MiniLightningPuff",55,45,0,0) 
    THOR T 1 BRIGHT A_CustomMissile("MiniLightningPuff",55,-45,0,0) 
    THOR T 1 BRIGHT A_CustomMissile("MiniLightningPuff",55,45,0,0) 
    THOR T 1 BRIGHT A_CustomMissile("MiniLightningPuff",55,-45,0,0) 
    THOR T 1 BRIGHT A_CustomMissile("MiniLightningPuff",55,45,0,0) 
    THOR T 1 BRIGHT A_CustomMissile("MiniLightningPuff",55,-45,0,0) 
    THOR T 1 BRIGHT A_CustomMissile("MiniLightningPuff",55,45,0,0) 
    THOR T 1 BRIGHT A_CustomMissile("MiniLightningPuff",55,-45,0,0) 
    THOR T 1 BRIGHT A_CustomMissile("MiniLightningPuff",55,45,0,0) 
    THOR T 1 BRIGHT A_CustomMissile("MiniLightningPuff",55,-45,0,0) 
    THOR T 1 BRIGHT A_CustomMissile("MiniLightningPuff",55,45,0,0) 
    THOR T 1 BRIGHT A_CustomMissile("MiniLightningPuff",55,-45,0,0)  
	TNT1 A 0 A_GiveInventory("ThorCounter", 1)
	TNT1 A 0 A_JumpIfInventory("ThorCounter", 9, "EndStream")
	Goto LightningStream+2
  EndStream:
	TNT1 A 0 A_TakeInventory("ThorCounter", 9)
	THOR T 5
    Goto See
//attack 3, stationary/invulnerable mode
  Filibuster:
    THOR ZY 5 A_FaceTarget
    THOR X 8 BRIGHT A_FaceTarget
    THOR Z 0 A_CustomMissile("GroundLightning",0,0,45,0,0)
    THOR Z 0 A_CustomMissile("GroundLightning",0,0,135,0,0)
    THOR Z 0 A_CustomMissile("GroundLightning",0,0,225,0,0)
    THOR Z 0 A_CustomMissile("GroundLightning",0,0,315,0,0)
    THOR Z 0 A_CustomMissile("GroundLightning",0,0,0,0,0)
    THOR Z 0 A_CustomMissile("GroundLightning",0,0,90,0,0)
    THOR Z 0 A_CustomMissile("GroundLightning",0,0,160,0,0)
    THOR Z 0 A_CustomMissile("GroundLightning",0,0,270,0,0)
    THOR Z 0 A_SetReflectiveInvulnerable
	TNT1 A 0 A_SetTranslucent(0.5, 1)
	TNT1 A 0 A_ChangeFlag("Visibilitypulse", 1)
	Goto FilibusterLoop
  FilibusterLoop:    
	THOR Z 35 A_Stop
    THOR RS 3 A_FaceTarget
    THOR T 8 BRIGHT A_CustomMissile("LightningRod1",55,45,0,0,0)
    THOR T 0 BRIGHT A_CustomMissile("LightningRod1",55,-45,0,0,0)
    THOR T 2 BRIGHT A_CustomMissile("LightningBallS",55,45,0,0,0)
    THOR T 0 BRIGHT A_CustomMissile("LightningBallS",55,-45,0,0,0)
    THOR T 2 BRIGHT A_CustomMissile("LightningBallS",55,45,0,0,0)
    THOR T 0 BRIGHT A_CustomMissile("LightningBallS",55,-45,0,0,0)
	TNT1 A 0 A_GiveInventory("ThorCounter", 1)
	TNT1 A 0 A_JumpIfInventory("ThorCounter", 3, "FilibusterEnd")
    THOR SR 3
    Goto FilibusterLoop
  FilibusterEnd:
	TNT1 A 0 A_UnSetReflectiveInvulnerable
	TNT1 A 0 A_SetTranslucent(1.0, 0)
	TNT1 A 0 A_ChangeFlag("Visibilitypulse", 0)
	TNT1 A 0 A_TakeInventory("ThorCounter", 9)
	THOR SR 3
    THOR YZM 10
    Goto See
//attack 4, floor-hugging seekers
  GroundSeekers:
    THOR ZY 5 A_FaceTarget
    THOR X 8 BRIGHT A_FaceTarget
    THOR X 0 BRIGHT A_CustomMissile("GroundLightningTracer",0,random(-20,20),random(-45,45),0,0)
    THOR X 0 BRIGHT A_CustomMissile("GroundLightningTracer",0,random(-20,20),random(-45,45),0,0)
    THOR X 0 BRIGHT A_CustomMissile("GroundLightningTracer",0,random(-20,20),random(-45,45),0,0)
	THOR X 8 Bright A_FaceTarget
    Goto See
//attack 5, lightning columns
  LightningColumns:
    THOR RS 5 A_FaceTarget
	TNT1 A 0 A_CustomMissile("LightningRod3", 0, 0, 0, CMF_AIMOFFSET)
	TNT1 A 0 A_CustomMissile("LightningRod3", 0, 45, random(-25, -15), CMF_AIMOFFSET)
	TNT1 A 0 A_CustomMissile("LightningRod3", 0, -45, random(15, 25), CMF_AIMOFFSET)
	THOR T 5 Bright A_FaceTarget
	THOR T 5
	Goto See
  Pain:
    THOR Q 10 A_Pain
	TNT1 A 0 A_TakeInventory("ThorCounter", 9)
    Goto See
  Pain.GravityHP:
    TNT1 A 0 ACS_NamedExecuteAlways("Hand-GravityAttack",0,0,0,0)
    Goto See
  Pain.GravityMP:
    TNT1 A 0 ACS_NamedExecuteAlways("Hand-GravityAttack",0,1,0,0)
    Goto See
  Pain.GravityHPMP:
    TNT1 A 0 ACS_NamedExecuteAlways("Hand-GravityAttack",0,2,0,0)
    Goto See
  Pain.Sceptre:
    TNT1 A 0 A_CheckFlag("FRIENDLY",2,AAPTR_DEFAULT)
    TNT1 A 0 ACS_NamedExecuteAlways("AddSceptreEXP",0,0,0,10) // Increase EXP for Sceptre [1+(HP/300)]
    TNT1 A 0 
    Goto Pain
  Death.Sceptre:
    TNT1 A 0 A_CheckFlag("FRIENDLY",2,AAPTR_DEFAULT)
    TNT1 A 0 ACS_NamedExecuteAlways("AddSceptreEXP",0,0,0,1) // Increase EXP for Sceptre [1+(HP/300)]
    TNT1 A 0 A_JumpIfHealthLower(user_HPXDeathRange,"XDeath")
    Goto Death
  Pain.Gauntlets:
    TNT1 A 0 ACS_NamedExecuteAlways("SauronGauntletsHit",0,0,1,0) // Give some life when hurting a monster [1+(HP/500)]
    Goto Pain
  Death.Gauntlets:
    TNT1 A 0 ACS_NamedExecuteAlways("SauronGauntletsHit",0,0,2,0) // Give some life when killing a monster [3+(HP/100)]
    TNT1 A 0 A_JumpIfHealthLower(user_HPXDeathRange,"XDeath")
    Goto Death
  Death:
    TNT1 A 0 ACS_NamedExecuteAlways("AddEXP",0,-2,0,1)
	TNT1 A 0 A_SetTranslucent(1.0, 0)
	TNT1 A 0 A_ChangeFlag("Visibilitypulse", 0)
    THR2 A 8 A_Scream
    THR2 B 8
    THR2 C 8
    THR2 D 6
    THR2 E 6
    THR2 F 6
    THR2 D 6
    THR2 E 6
    THR2 F 6
    THR2 D 6
    THR2 E 6
    THR2 F 6
    THR2 G 4 BRIGHT A_PlaySound("thor/thunder")
    THR2 H 6 A_NoBlocking
    THR2 I 6
    THR2 J 6
    THR2 K 6
    THR2 L 6
    THR2 M 6
    THR2 N 6
    TNT1 A 0 ACS_NamedExecuteAlways("MonsterStatsHP_ATK_DEF_SPD",0,1)
	TNT1 A 0 A_SpawnItemEx("ThorCorpse", 0, 0, 0, 0, 0, 0, 0, SXF_NOCHECKPOSITION)
	//note that he needs to Stop and "disappear" to shut off his orbiters; a dummy corpse is left instead (see below)
    Stop
  }
}

//used to manage Thor's "looping" attacks
Actor ThorCounter : Inventory
{
  Inventory.MaxAmount 9
}

//dummy corpse
Actor ThorCorpse
{
  Radius 25
  Height 12
  States
  {
  Spawn:
    THR2 O -1
    Loop
  }
}

//orbiters
ACTOR LightningBallC1
{
     +Noclip
     Scale .35
     Speed 5
     RENDERSTYLE Add
     Alpha 0.50     
     PROJECTILE
     states
     {
     Spawn:
		 ZOT3 AABBCC 1 BRIGHT A_SpawnItemEx("LightningBallC1T", 0, 0, 0, 0, 0, 0, 0, 128)
         ZOT3 A 0 A_CustomMissile("LightningBallC1", 0, 0, 75)
         ZOT3 A 0 A_MissileAttack
         Stop
	 Death:
		 TNT1 A 0
		 Stop
     }
}

