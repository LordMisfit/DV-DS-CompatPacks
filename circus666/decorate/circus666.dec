  States
  {
  Spawn:
    JUMP A 0 NoDelay ACS_NamedExecuteAlways("MonsterStatsHP_ATK_DEF_SPD",0,0)
    JUMP A 4 A_Look
    goto Spawn+1
  See:
    JUMP A 0 ACS_NamedExecuteAlways("RefreshStealthInfo",0,0)
    JUMP ABCB 2
	{
     if (GetCVar("dvds_monsterlevelbehavior") > 0 && user_monsterlevel > 120) { A_SetTics(1); }
