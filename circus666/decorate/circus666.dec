        RRCK A 1 A_CustomMissile("RevTrail", 0, 0, 0, 1)  
        RRCK A 1 A_CustomMissile("RevTrail", 0, 0, 0, 1)  
        RRCK A 1 A_CustomMissile("RevTrail", 0, 0, 0, 1)  
        RRCK A 1 A_CustomMissile("RevTrail", 0, 0, 0, 1)  
        Loop
    Death:
        MISL ABC 4 Bright A_Explode(12,96)
        Stop
    }
}

actor RevRocketCrisisParticle : BaseCrisisParticle
{
 scale 0.3666666666666667
 
 States
 {
  Spawn:
    RRCK A 0
    "####" A 2 Bright
	{
     A_SetScale(scaleX-0.055,scaleY-0.055);
	 if (scalex < 0.00 && scaley < 0.00) { A_FadeOut(2); }
	 A_FadeOut(0.08);
	}
    loop
 }
}

actor RevRocketCrisis : RevRocket
{
    Radius 8
    Height 6
    Speed 12
    Damage ((2 * ACS_NamedExecuteWithResult("CrisisMultiplier")) * random(1,8))
    Scale .55
    +SEEKERMISSILE
    PROJECTILE
    SeeSound "REVRCK"
    DeathSound "explode"
    ExplosionRadius 96
    ExplosionDamage 12
    States
    {
    Spawn:
        RRCK A 1 
		{
         A_SpawnItemEx("RevRocketCrisisParticle",0,0,0,0,0,1.28);
         A_SeekerMissile(30,40);
		}
        RRCK A 1
		{
         A_SpawnItemEx("RevRocketCrisisParticle",0,0,0,0,0,1.28);
         A_CustomMissile("RevTrail", 0, 0, 0);
		}
        RRCK A 1 
		{
         A_SpawnItemEx("RevRocketCrisisParticle",0,0,0,0,0,1.28);
         A_PlaySound("HISS");
		}
        RRCK AAAAAAAAA 1 
		{
         A_SpawnItemEx("RevRocketCrisisParticle",0,0,0,0,0,1.28);
         A_CustomMissile("RevTrail", 0, 0, 0, 1);
		}
        Loop
    Death:
        MISL ABC 4 Bright A_Explode((12 * ACS_NamedExecuteWithResult("CrisisMultiplier")),96)
        Stop
    }
}

