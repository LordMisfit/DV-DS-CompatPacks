	GLAG B 0 BRIGHT A_GunFlash
	GLAG B 0 BRIGHT A_PlaySound("weapons/grenadefire")
	GLAG B 4 BRIGHT
	{
     ACS_NamedExecuteAlways("StartTensionCountDown");
	 if (CountInv("SpreaderAura2",AAPTR_PLAYER1) > 0)
	 {
      // Angle + 15, Pitch + 0
      A_SetAngle(angle+15);
	  A_FireProjectile("KDiZDGrenade",0,0,0,0);
      // Angle + 7.5, Pitch + 7.5
      A_SetAngle(angle-7.5);
      A_SetPitch(pitch+7.5);
	  A_FireProjectile("KDiZDGrenade",0,0,0,0);
      // Angle + 7.5, Pitch - 7.5
      A_SetPitch(pitch-15);
	  A_FireProjectile("KDiZDGrenade",0,0,0,0);
      // Angle + 0, Pitch + 0
      A_SetPitch(pitch+7.5);
      A_SetAngle(angle-7.5);
	  A_FireProjectile("KDiZDGrenade",0,1,0,0);
      // Angle - 7.5, Pitch - 7.5
      A_SetAngle(angle-7.5);
      A_SetPitch(pitch-7.5);
	  A_FireProjectile("KDiZDGrenade",0,0,0,0);
      // Angle - 7.5, Pitch + 7.5
      A_SetPitch(pitch+15);
	  A_FireProjectile("KDiZDGrenade",0,0,0,0);
      // Angle - 15, Pitch + 0
      A_SetPitch(pitch-7.5);
      A_SetAngle(angle-7.5);
	  A_FireProjectile("KDiZDGrenade",0,0,0,0);
      // Reset
      A_SetAngle(angle+15);
	 }
	 else
	 if (CountInv("SpreaderAura1",AAPTR_PLAYER1) > 0)
	 {
      A_SetAngle(angle+15);
	  A_FireProjectile("KDiZDGrenade",0,0,0,0);
      A_SetAngle(angle-15);
	  A_FireProjectile("KDiZDGrenade",0,1,0,0);
      A_SetAngle(angle-15);
	  A_FireProjectile("KDiZDGrenade",0,0,0,0);
      A_SetAngle(angle+15);
	 }
	 else
	 {
	  A_FireProjectile("KDiZDGrenade",0,1,0,0);
	 }
	}
	GLAG I 4
	GLAG A 0 A_CheckReload //I can't believe I forgot this.
	GLAG C 7
	GLAG D 5
	GLAG E 5 A_PlaySound("weapons/sshoto")
	GLAG F 5
	GLAG G 5 A_FireProjectile("GrnCaseSpawn",0,0,-4,-4)
	GLAG H 5 A_PlaySound("weapons/sshotl")
	GLAG FE 5
	GLAG D 5 A_PlaySound("weapons/sshotc")
	GLAG A 3
	GLAG A 0 A_Refire
	Goto Ready
  AltFire:
	GLAG B 0 BRIGHT A_GunFlash
	GLAG B 0 BRIGHT A_PlaySound("weapons/grenadefire")
	GLAG B 4 BRIGHT A_FireProjectile("KDiZDGrenade2",0,1,0,0)
	GLAG I 4
	GLAG A 0 A_CheckReload //I can't believe I forgot this.
	GLAG C 7
	GLAG D 5
	GLAG E 5 A_PlaySound("weapons/sshoto")
	GLAG F 5
	GLAG G 5 A_FireProjectile("GrnCaseSpawn",0,0,-4,-4)
	GLAG H 5 A_PlaySound("weapons/sshotl")
	GLAG FE 5
	GLAG D 5 A_PlaySound("weapons/sshotc")
	GLAG A 3
	GLAG A 0 A_Refire
	Goto Ready
  Flash:
   	TNT1 A 2 BRIGHT A_Light1
   	TNT1 A 2 BRIGHT A_Light2
   	TNT1 A 0 A_Light0
   	Stop
  }
}


