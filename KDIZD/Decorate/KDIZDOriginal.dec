			Loop

		Select:
			TNT1 A 0 
			{
				if (CallACS("MiscVarCheckDECORATE",10019) >= 2) { A_Raise; }
				if (CallACS("MiscVarCheckDECORATE",10051) >= 3) { A_Raise; }
				if (CallACS("MiscVarCheckDECORATE",10019) >= 4) { A_Raise; }
			}
			TNT1 A 0 { A_Raise; A_Raise; }
			RIFG A 1 A_Raise
			Loop
		Deselect:
			TNT1 A 0 
			{
				if (CallACS("MiscVarCheckDECORATE",10019) >= 2) { A_Lower; }
				if (CallACS("MiscVarCheckDECORATE",10051) >= 3) { A_Lower; }
				if (CallACS("MiscVarCheckDECORATE",10019) >= 4) { A_Lower; }
			}
			TNT1 A 0 { A_Lower; A_Lower; }
			RIFG A 1 A_Lower
			Loop
		Ready:
			RIFG A 1 A_WeaponReady(WRF_ALLOWUSER1)
			Loop
		Fire:
			RIFG A 0 A_GunFlash
			RIFG A 0 A_FireCustomMissile("RifCaseSpawn",0,0,2,-2)
			RIFG A 0 
			RIFG A 4
			{
				CallACS("StartTensionCountDown");
				A_GetPlayerAttackDamage(60,1);
				A_SpreadAuraFireBullets(0,0,1,(CountInv("FinalDamageDealt",AAPTR_PLAYER1)),"NewPuff",FBF_NORANDOM,FBF_USEAMMO|FBF_NORANDOM,0,"",0,0,1,"RifleAmmo");
			}
			RIFG BCDCB 2
			RIFG A 3
			RIFG A 0 A_Refire
			Goto Ready
		Flash:
			RIFF A 2 BRIGHT A_Light1
			RIFF B 2 BRIGHT A_Light2
			TNT1 A 0 A_Light0
			Stop
	}
}

