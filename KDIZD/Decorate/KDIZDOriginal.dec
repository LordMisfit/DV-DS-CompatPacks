ACTOR EvilWarriorShield : KDIZDWeapon
{
  Radius 8
  Height 8
  Speed 6
  Weapon.SlotNumber 9
  Weapon.SlotPriority 0.5
  Weapon.BobSpeed 2.0
  Weapon.BobStyle Alpha
  Weapon.KickBack 0
  //Weapon.UpSound "weapons/evilwarriorshield/up"
  Inventory.PickupMessage "You got the Hell Warrior's Shield!"
  Obituary "%o was ripped apart by %k's Shield."
  BounceType Doom
  +WEAPON.NOALERT
  +DROPOFF
  Tag "Hell Warrior's Shield"
  States
  {
  Ready:
	EWSP D 1 A_WeaponReady(WRF_ALLOWUSER1)
	Goto Ready
  Select:
	EWSP DDD 0
	{
	 A_GiveInventory("EWShieldIdleMode",1);
	 A_Raise;
	}
	EWSP D 1
	{
	 A_GiveInventory("EWShieldIdleMode",1);
	 A_Raise;
	}
	Loop
  Deselect:
	EWSP DDD 0
	{
	 A_TakeInventory("EWShieldIdleMode",9999);
	 A_Lower;
	}
	EWSP D 1
	{
	 A_TakeInventory("EWShieldIdleMode",9999);
	 A_Lower;
	}
	Loop
  Fire:
    TNT1 A 0 A_JumpIf(ACS_NamedExecuteWithResult("UseManaAttack",2,2) == 0, "Ready")
	EWSP D 2 Bright
	{
	 if (ACS_NamedExecuteWithResult("MiscVarCheckDECORATE",10053) >= 5) { A_SetTics(1); }
	 A_ChangeFlag("NOPAIN", 1);
	 A_ChangeFlag("NODAMAGE", 1);
	 A_ChangeFlag("REFLECTIVE", 1);
 	 A_ChangeFlag("SHIELDREFLECT", 1);
	}
	EWSP C 2 Bright
	{
	 if (ACS_NamedExecuteWithResult("MiscVarCheckDECORATE",10053) >= 3) { A_SetTics(1); }
	}
	EWSP B 2 Bright
	{
	 if (ACS_NamedExecuteWithResult("MiscVarCheckDECORATE",10053) >= 1) { A_SetTics(1); }
	}
	EWSP A 6 Bright
	{
	 A_SetPitch(pitch + 0.5);
	 if (ACS_NamedExecuteWithResult("MiscVarCheckDECORATE",10053) >= 2) { A_SetTics(5); }
	 if (ACS_NamedExecuteWithResult("MiscVarCheckDECORATE",10053) >= 4) { A_SetTics(4); }
	}
	EWSP A 2 Bright
	{
	 if (ACS_NamedExecuteWithResult("MiscVarCheckDECORATE",10053) >= 3) { A_SetTics(1); }
   	 ACS_NamedExecuteAlways("StartTensionCountDown");
	 if (CountInv("SpreaderAura2",AAPTR_PLAYER1) > 0)
	 {
      A_FireProjectile("EvilWarriorShieldBall",15,0,0,3,0,0); // Left
      A_FireProjectile("EvilWarriorShieldBall",7.5,0,0,3,0,7.5); // Left-Top
      A_FireProjectile("EvilWarriorShieldBall",7.5,0,0,3,0,-7.5); // Left-Bottom
      A_FireProjectile("EvilWarriorShieldBall",0,0,0,3,0,0); // Center
      A_FireProjectile("EvilWarriorShieldBall",-7.5,0,0,3,0,-7.5); // Right-Bottom
      A_FireProjectile("EvilWarriorShieldBall",-7.5,0,0,3,0,7.5); // Right-Top
      A_FireProjectile("EvilWarriorShieldBall",-15,0,0,3,0,0); // Right
	 }
	 else
	 if (CountInv("SpreaderAura1",AAPTR_PLAYER1) > 0)
	 {
      A_FireProjectile("EvilWarriorShieldBall", 15, 0, 0, 3);
      A_FireProjectile("EvilWarriorShieldBall",  0, 0, 0, 3);
      A_FireProjectile("EvilWarriorShieldBall",-15, 0, 0, 3);
	 }
	 else
	 {
      A_FireProjectile("EvilWarriorShieldBall", 0, 0, 0, 3);
	 }
     if (random(1,8) == 1) { A_AlertMonsters; }
	 A_Recoil(0.5);
	}
	EWSP A 6 Bright
	{
	 A_SetPitch(pitch - 0.5);
	 if (ACS_NamedExecuteWithResult("MiscVarCheckDECORATE",10053) >= 2) { A_SetTics(5); }
	 if (ACS_NamedExecuteWithResult("MiscVarCheckDECORATE",10053) >= 4) { A_SetTics(4); }
	}
	EWSP B 2 Bright
	{
	 if (ACS_NamedExecuteWithResult("MiscVarCheckDECORATE",10053) >= 1) { A_SetTics(1); }
	}
	EWSP C 2 Bright
	{
	 if (ACS_NamedExecuteWithResult("MiscVarCheckDECORATE",10053) >= 3) { A_SetTics(1); }
	}
	EWSP D 2 Bright
	{
	 if (ACS_NamedExecuteWithResult("MiscVarCheckDECORATE",10053) >= 5) { A_SetTics(1); }
	 A_ChangeFlag("NOPAIN", 0);
	 A_ChangeFlag("NODAMAGE", 0);
	 A_ChangeFlag("REFLECTIVE", 0);
 	 A_ChangeFlag("SHIELDREFLECT", 0);
	}
	Goto Ready
  AltFire:
	EWSP D 2
	{
	 A_ChangeFlag("NOPAIN", 1);
	 A_ChangeFlag("NODAMAGE", 1);
	 A_ChangeFlag("REFLECTIVE", 1);
 	 A_ChangeFlag("SHIELDREFLECT", 1);
	}
	EWSP E 2
	EWSP F 2
  AltHold:
	EWSP G 2 BRIGHT
	EWSP G 0 A_Refire("AltHold")
	goto AltEnd
  AltEnd:
	EWSP F 2
	EWSP E 2
	EWSP D 2
	{
	 A_ChangeFlag("NOPAIN", 0);
	 A_ChangeFlag("NODAMAGE", 0);
	 A_ChangeFlag("REFLECTIVE", 0);
 	 A_ChangeFlag("SHIELDREFLECT", 0);
	}
	Goto Ready
  Spawn:
	EWSH ABCDEFGH 3
	Goto Spawn2
  Spawn2:
	EWSH I -1
	Stop
  }
}

