	  TRO4 EF 5 A_FaceTarget
	  TRO4 G 4 A_CustomComboAttack("DoomedImpBall",45,6,"imp/melee")
	  TRO4 EF 5 A_FaceTarget
	  TRO4 G 4 A_CustomComboAttack("DoomedImpBall",45,6,"imp/melee")
      Goto See
	LightningBall:
      TRO4 G 6 A_CustomComboAttack("DoomedImpLightningBall",45,6,"imp/melee")
	  TRO4 EF 5 A_FaceTarget
	  TRO4 G 4 A_CustomComboAttack("DoomedImpLightningBall",45,6,"imp/melee")
	  TRO4 EF 5 A_FaceTarget
	  TRO4 G 4 A_CustomComboAttack("DoomedImpLightningBall",45,6,"imp/melee")
	  Goto See
    Pain:
      TRO4 H 2
      TRO4 H 2 A_Pain
      Goto See
    Death:
      TRO4 I 8
      TRO4 J 8 A_Scream
      TRO4 K 6
      TRO4 L 6 A_NoBlocking
      TRO4 M -1
      Stop
    XDeath:
      TROO N 5
      TROO O 5 A_XScream
      TROO P 5
      TROO Q 5 A_NoBlocking
      TROO RST 5
      TROO U -1
      Stop
    Raise:
      TRO4 ML 8
      TRO4 KJI 6
      Goto See
    }
} 

