ACTOR InfernoViscount 18022
{
//$Category "ZDoom Monster pack"
   spawnid 255
   Health 1500
   Radius 36
   Height 96
   Mass 1800
   Speed 8
   PainChance 35
   Monster
   Scale 1.25
   +FLOORCLIP
   +MISSILEEVENMORE
   BloodColor "Green"
   SeeSound "baron/sight"
   PainSound "baron/pain"
   DeathSound "baron/death"
   ActiveSound "baron/active"
   Translation "16:47=96:111", "112:127=182:191"
   BloodColor "FF0900"
   Obituary "%o was slain by an inferno viscount."
   States
   {
   Spawn:
      BOSS AB 10 A_Look 
      Loop
   See:
      BOSS AABBCCDD 4 A_Chase
      Loop
   Missile:
      BOSS EF 6 A_FaceTarget
      BOSS G 0 A_CustomMissile("BaronBall2",45,0,-20+(random(5,-5)))
      BOSS G 0 A_CustomMissile("BaronBall2",45,0,20+(random(5,-5)))
      BOSS G 6 A_CustomMissile("BaronBall2Big",45,0,0)
      Goto See
   Pain:
      BOSS H  2
      BOSS H  2 A_Pain
      Goto See
	Pain.Stunner:
	BOSS H 2 A_ChangeFlag("NoPain", 1)
	BOSS H 300 A_Pain
	BOSS H 0 A_ChangeFlag("NoPain", 0)
	goto See
   Death:
      BOSS I  8
      BOSS J  8 A_Scream
      BOSS K  8
      BOSS L  8 A_NoBlocking
      BOSS MN 8
      BOSS O -1 A_BossDeath   
      Stop
   Raise:
      BOSS O 8
      BOSS NMLKJI  8
      Goto See
  Pain.Cube:
    TNT1 A 0 A_GiveToTarget ("Stimpack", 1)
    Goto Pain+1
  Death.Cube:
    TNT1 A 0 A_GiveToTarget ("Stimpack", 2)
    Goto Death+1
   }
}

